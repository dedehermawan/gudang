<div class="container">
  <div class="well">
    <%= link_to "New Division", new_division_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-division"></div>

  <table class="table table-bordered table-stripped">
    <thead>
      <tr>
        <td>Division ID</td>
        <td>Division Name</td>
        <td colspan="2" class="text-center">Action</td>
      </tr>
    </thead>
    <tbody class="division-index">
      <%= render 'index' %>
    </tbody>
  </table>
</div>
<div id="division-modal" class="modal fade"></div>


