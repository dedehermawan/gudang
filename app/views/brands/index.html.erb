<div class="container">
  <div class="well">
    <%= link_to "New Brand", new_brand_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-brands"></div>

  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <td>No.</td>
        <td>Brand Name</td>
        <td colspan="2" class="text-center">Action</td>
      </tr>
    </thead>
    <tbody class="brand-index">
      <%= render 'index' %>
    </tbody>
  </table>
</div>
<div id="brand-modal" class="modal fade"></div>

