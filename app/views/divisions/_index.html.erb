<% @divisions.each do |division| %>
  <tr>
    <td><%= division.division_id %></td>
    <td><%= division.division_name %></td>
    <td class="text-center">
      <%= link_to 'Edit', edit_division_path(division), remote: true, class: 'btn btn-default' %>
      <%= link_to "Delete", division_delete_path(division), remote: true, class: 'btn btn-danger' %>
    </td>
  </tr>
<% end %>
