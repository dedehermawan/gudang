<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <h3>Delete Coa</h3>
    </div>
    <div class="modal-body">
      <ul class="errors"></ul>
      Are you sure you wish to delete <b><%= @coa.coa_name %></b>?
    </div>
    <div class="modal-footer">
      <%= link_to "Yes, Delete This Coa", coa_path(@coa), method: :delete, remote: true, class: "btn btn-danger" %>
      <%= link_to "No, Please Don't", "#", class: "btn btn-default", data: { dismiss: "modal" } %>
    </div>
  </div>
</div>
