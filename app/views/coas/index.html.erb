<div class="container">
  <div class="well">
    <%= link_to "New Coa", new_coa_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-coa"></div>

  <table class="table table-bordered table-stripped">
    <thead>
      <tr>
        <td>Coa ID</td>
        <td>Coa Name</td>
        <td colspan="2" class="text-center">Action</td>
      </tr>
    </thead>
    <tbody class="coa-index">
      <%= render 'index' %>
    </tbody>
  </table>
</div>
<div id="coa-modal" class="modal fade"></div>


